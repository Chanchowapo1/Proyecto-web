---
import Layout from "../layouts/Layout.astro"
---
<header class="barranav">
    <nav class="max-w-[85rem] w-full mx-auto px-4 flex items-center justify-between" aria-label="Global">
        <div class="flex items-center">
            <img src="logo naranja 1.png" alt="Logo de CCI Condal Barcelona" class="h-8">
            <a class="text-xl font-semibold dark:text-white ml-2" href="#">CCI Condal Barcelona </a>
        </div>
        <div class="flex items-center gap-5">
            <a class="font-medium dark:text-gray-400 dark:hover:text-gray-500 dark:focus:outline-none dark:focus:ring-1 dark:focus:ring-gray-600" href="#">Inicio</a>
            <a class="font-medium text-gray-600 hover:text-gray-400 dark:text-gray-400 dark:hover:text-gray-500 dark:focus:outline-none dark:focus:ring-1 dark:focus:ring-gray-600" href="#">Oracion</a>
            <a class="font-medium text-gray-600 hover:text-gray-400 dark:text-gray-400 dark:hover:text-gray-500 dark:focus:outline-none dark:focus:ring-1 dark:focus:ring-gray-600" href="#">Iglesia</a>
            <a class="font-medium text-gray-600 hover:text-gray-400 dark:text-gray-400 dark:hover:text-gray-500 dark:focus:outline-none dark:focus:ring-1 dark:focus:ring-gray-600" href="#">Contacto</a>
        </div>
    </nav>
</header>
